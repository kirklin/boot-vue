{
  "name": "boot-vue",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.8.1",
  "description": "Vue starter template with UnoCSS",
  "keywords": [
    "BootVue",
    "boot-vue",
    "vite",
    "unocss",
    "fast",
    "boot"
  ],
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "npx husky",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@kirklin/logger": "catalog:",
    "@kirklin/reset-css": "catalog:",
    "@vueuse/core": "catalog:",
    "daisyui": "catalog:",
    "iso-639-1": "catalog:",
    "nprogress": "catalog:",
    "pinia": "catalog:",
    "vue": "catalog:",
    "vue-i18n": "catalog:",
    "vue-router": "catalog:"
  },
  "devDependencies": {
    "@commitlint/cli": "catalog:",
    "@commitlint/config-conventional": "catalog:",
    "@iconify-json/mdi": "catalog:",
    "@iconify-json/tabler": "catalog:",
    "@intlify/unplugin-vue-i18n": "catalog:",
    "@kirklin/eslint-config": "catalog:",
    "@types/node": "catalog:",
    "@types/nprogress": "catalog:",
    "@unocss/eslint-config": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vitest/coverage-c8": "catalog:",
    "@vue/compiler-dom": "catalog:",
    "@vue/test-utils": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-format": "catalog:",
    "eslint-plugin-vuejs-accessibility": "catalog:",
    "husky": "catalog:",
    "jsdom": "catalog:",
    "lint-staged": "catalog:",
    "typescript": "catalog:",
    "unocss": "catalog:",
    "unocss-preset-chinese": "catalog:",
    "unocss-preset-daisy": "catalog:",
    "unocss-preset-ease": "catalog:",
    "unplugin-auto-import": "catalog:",
    "unplugin-icons": "catalog:",
    "unplugin-vue-components": "catalog:",
    "vite": "catalog:",
    "vite-plugin-inspect": "catalog:",
    "vite-plugin-pwa": "catalog:",
    "vite-plugin-vue-devtools": "catalog:",
    "vitest": "catalog:",
    "vue-tsc": "catalog:"
  },
  "lint-staged": {
    "**/*": "eslint --fix"
  }
}
