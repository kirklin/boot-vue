{
  "name": "boot-vue",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.2.1",
  "description": "Vue starter template with UnoCSS",
  "keywords": [
    "BootVue",
    "boot-vue",
    "vite",
    "unocss",
    "fast",
    "boot"
  ],
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "npx husky",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@kirklin/logger": "^0.0.2",
    "@kirklin/reset-css": "^0.0.6",
    "@vueuse/core": "^12.5.0",
    "daisyui": "3.9.4",
    "iso-639-1": "^3.1.5",
    "nprogress": "^0.2.0",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/tabler": "^1.2.16",
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@kirklin/eslint-config": "^3.0.0",
    "@types/node": "^22.13.1",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^65.4.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/compiler-dom": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.20.0",
    "eslint-plugin-format": "^1.0.1",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "typescript": "^5.7.3",
    "unocss": "^65.4.3",
    "unocss-preset-chinese": "^0.3.3",
    "unocss-preset-daisy": "^7.0.0",
    "unocss-preset-ease": "^0.0.4",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-icons": "^22.0.0",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^6.1.0",
    "vite-plugin-inspect": "^10.1.1",
    "vite-plugin-pwa": "^0.21.1",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.0"
  },
  "lint-staged": {
    "**/*": "eslint --fix"
  }
}
