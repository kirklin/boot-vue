{
  "name": "boot-vue",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.4.0",
  "description": "Vue starter template with UnoCSS",
  "keywords": [
    "BootVue",
    "boot-vue",
    "vite",
    "unocss",
    "fast",
    "boot"
  ],
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@kirklin/logger": "^0.0.2",
    "@kirklin/reset-css": "^0.0.6",
    "@vueuse/core": "^10.11.0",
    "daisyui": "^3.9.4",
    "iso-639-1": "^3.1.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.30",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@iconify-json/mdi": "^1.1.67",
    "@iconify-json/tabler": "^1.1.114",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@kirklin/eslint-config": "^2.4.0",
    "@types/node": "^20.14.8",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.61.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/compiler-dom": "^3.4.30",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.5.0",
    "eslint-plugin-format": "^0.1.2",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "typescript": "^5.5.2",
    "unocss": "^0.61.0",
    "unocss-preset-chinese": "^0.3.2",
    "unocss-preset-daisy": "^7.0.0",
    "unocss-preset-ease": "^0.0.3",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-icons": "^0.19.0",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.3.1",
    "vite-plugin-inspect": "^0.8.4",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-vue-devtools": "^7.3.4",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.22"
  },
  "lint-staged": {
    "**/*": "eslint --fix"
  }
}
