{
  "name": "vite-boot",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.5.1",
  "description": "Vite starter template with UnoCSS",
  "keywords": [
    "viteBoot",
    "vite-boot",
    "vite",
    "unocss",
    "fast",
    "boot"
  ],
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@kirklin/logger": "^0.0.2",
    "@kirklin/reset-css": "^0.0.3",
    "@vueuse/core": "^10.1.2",
    "daisyui": "^2.51.6",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.36",
    "unocss-preset-chinese": "^0.0.6",
    "vue": "^3.3.2",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@iconify-json/mdi": "^1.1.51",
    "@iconify-json/tabler": "^1.1.75",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@kirklin/eslint-config": "^0.3.6",
    "@types/node": "^20.1.4",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-c8": "^0.31.0",
    "@vue/compiler-dom": "^3.3.2",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.40.0",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.2",
    "pnpm": "^8.5.1",
    "typescript": "^5.0.4",
    "unocss": "^0.51.12",
    "unocss-preset-daisy": "^4.0.0",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.5",
    "vite-plugin-inspect": "^0.7.26",
    "vite-plugin-vue-devtools": "^0.0.16",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.5"
  },
  "lint-staged": {
    "**/*": "eslint --fix"
  }
}
